    "/attendances/{id}": {
      "patch": {
        "summary": "Updates an attendance",
        "tags": [
          "Attendances"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "Attendance ID",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "Authorization",
            "in": "header",
            "value": "Bearer <JWT Token>",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "examples": {
                  "update_attendances": {
                    "value": {
                      "id": 1339,
                      "status": "accepted",
                      "session_id": 702,
                      "member_id": 2118
                    }
                  }
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "string"
                  }
                },
                "required": [
                  "attendance"
                ]
              }
            }
          }
        }
      }
    },
